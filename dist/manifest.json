{
  "name": "Ticket History",
  "author": {
    "name": "Zena Hirsch",
    "email": "zena@vimeo.com",
    "url": "https://github.com/vimeo/zendesk-ticket-history/issues"
  },
  "defaultLocale": "en",
  "private": false,
  "location": {
    "support": {
      "ticket_sidebar": "assets/index.html"
    }
  },
  "parameters": [
    {
      "name": "list_length",
      "type": "number",
      "default": 5
    },
    {
      "name": "show_satisfaction",
      "type": "checkbox",
      "default": true
    },
    {
      "name": "show_preview",
      "type": "checkbox",
      "required": true
    }
  ],
  "version": "1.0.4",
  "frameworkVersion": "2.0"
}
